int _strlen_recursion(char *s)
{
	int len;

	len = 0;
	if (*s != '\0')
	{
		len += 4;
		printf("%c\n", *s)<F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F<F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10<F10><F10><F10><F10><F10><F10<F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F1<F10><F10><F10><F10><F10><F10><F10><F10><F10<F10><F10><F10><F10><F10><F10><F10><F10><F10><F10><F10<F10><F10><F10><F10><F10><F10><F10><F10><F10<F10><F10><F10><F10><F10><F10><F10<F10><F10><F10><F10><F10><F10><F10<F10><F10><F10><F10><F10><F10><F10>0>0><F10><F10><F10><F10>;
		s++;
		_strlen_recursion (s);
	}
	return (len);
}
